    <string-array name="flip_action_summary_entries">
        <item>Fazer nada</item>
        <item>Silenciar</item>
        <item>Recusar chamada</item>
    </string-array>

    <string name="calllog_after_call">Registro de chamadas após uma chamada</string>
    <string name="calllog_after_call_summary">Mostrar o registro de chamadas depois de cada chamada</string>

