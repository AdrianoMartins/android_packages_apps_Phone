    <!-- Setting title for the flip action setting. -->
    <string name="flip_action_title">Virar o Telefone</string>

    <!-- Dialog title of the flip action setting. -->
    <string name="flip_action_dialog_title">Vire o telefone para\u2026</string>

    <!-- Setting summary for the flip action setting. -->
    <string name="flip_action_summary">Virando o telefone para baixo deve <xliff:g id="action">%s</xliff:g></string>

    <!-- Values for the flip action settings. -->
    <string-array name="flip_action_setting_values" translatable="false">
        <item>0</item>
        <item>1</item>
        <item>2</item>
    </string-array>

    <!-- Entries listed in the setting for the flip action. -->
