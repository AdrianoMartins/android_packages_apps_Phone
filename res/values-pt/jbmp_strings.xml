    <string name="blacklist_title">Lista Negra</string>
    <string name="enable_blacklist_title">Habiliar Lista Negra</string>
    <string name="blacklist_summary_disabled">Desativar</string>
    <string name="blacklist_summary">As chamadas recebidas de números de telefone na lista negra serão bloqueados</string>
    <string name="blacklist_notify">Mostrar notificação</string>
